{
  "name": "web-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "watch-build": "tsc --watch",
    "watch-test": "vitest --watch",
    "test": "vitest --run",
    "test:report": "vitest --run --reporter=verbose,dot",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "watch": "concurrently --names 'TS,JEST' -c 'red,blue' \"npm run watch-build\" \"npm run watch-test\"",
    "test:e2e": "playwright test",
    "sentry:release": "sentry-cli releases new $npm_package_version && sentry-cli releases set-commits $npm_package_version --auto && sentry-cli releases finalize $npm_package_version"

  },
  "dependencies": {
